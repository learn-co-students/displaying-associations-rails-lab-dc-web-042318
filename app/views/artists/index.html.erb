<% @artists.each do |artist| %>
     <p><%= link_to "#{artist.name}", artist_path(artist) %></p>
     <p><%= pluralize(artist.song_count, 'song') %>   
        <ul>
            <% artist.songs.each do |song| %>
                <li><%= link_to "#{song.title}", song_path(song)%></li>
            <% end %>
        </ul>
<% end %>